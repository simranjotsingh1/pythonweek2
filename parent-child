class pigeon:
    def __init__(self,age,weight):
        self.age = age
        self.weight = weight
    def fly(self):
         print("i am flying now ..")

    def land(self):
        print("i am landed!!")

class Eagle(pigeon):
    def __init__(self,age,weight,attackspeed):
        self.age = age
        self.weight = weight
        self.attackspeed = attackspeed

    def attack(self):
        print("Attacking with the speed of " + str(self.attackspeed))
e1 = Eagle(12, 30, 46)
e1.fly()
e1.land()
e1.attack()
